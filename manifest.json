{
  "name": "Autofill Factorial attendance shifts",
  "version": "1.0",
  "description": "Autofills Factorial attendance shiflts",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_icon": "icon-32.png",
    "default_title": "Autofill factorial attendance shifts",
    "default_popup": "popup.html"
  },
  "permissions": [
    "declarativeContent",
    "storage",
    "tabs",
    "webRequest"
  ],
  "host_permissions": [
    "*://*.factorialhr.com/"
  ],
  "icons": {
    "32": "icon-32.png",
    "128": "icon-128.png"
  },
  "manifest_version": 3
}
